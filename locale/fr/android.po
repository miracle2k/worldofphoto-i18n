msgstr "%s vous demande de prendre une photo."

#, python-format
msgctxt "notify_make_photo_msg"
msgid "You promised %s a photo."
msgstr "Vous avez promis une photo à %s."

msgctxt "notify_spin_msg"
msgid "It is now your turn to spin."
msgstr "C'est à votre tour de \"faire tourner\" !"

#, python-format
msgctxt "notify_message_received_msg"
msgid "%s has sent you a message."
msgstr "Vous avez reçu un message de %s."

#, python-format

msgstr "Inserisci il tuo nome utente."

msgctxt "password_missing_register"
msgid "Please choose a password."
msgstr "Scegli una parola chiave."

msgctxt "password_missing_login"
msgid "Please enter your password."
msgstr "Inserisci la tua parola chiave."

msgctxt "passwords_not_matching"
msgid "Passwords do not match."
msgstr "Le parole chiave non corrispondono."

msgctxt "login_failed"
msgid "Your username or password was incorrect."
msgstr "Il tuo nome utente o parola chiave non sono corretti."

msgctxt "register_failed"
msgid "Sorry, the username you have chosen is not available."
msgstr "Spiacente, il nome utente scelto non é disponibile."

msgctxt "onboot_pref_title"
msgid "Resume on boot"
msgstr "Continua dopo il riavvio"

msgctxt "onboot_pref_summary"
msgid "Automatically connect on boot if connected before shutdown."
msgstr ""
"Collegati automaticamente dopo in riavvío, se eri collegato prima dello "
"spegnimento."

msgctxt "compass_spin_pref_title"
msgid "Compass Spin enabled"
msgstr "Rotazione bussola abilitata"

msgctxt "compass_spin_pref_summary"
msgid "Disable if it makes problems. It's a gimmick anyway!"
msgstr "Disabilita se crea problemi. É solo un espediente!"

msgctxt "selection_freq_pref_title"
msgid "Selection Frequency"
msgstr "Frequenza di selezione"

msgctxt "selection_freq_pref_summary"
msgid "Just as a recommendation to the server. No guarantees!"
msgstr "É solo una raccomandazione per il server. Non ci sono garanzie!"

msgctxt "selection_freq_value_half"
msgid "Once every half hour"
msgstr "Ona volta ogni mezz'ora"

msgctxt "selection_freq_value_one"
msgid "Once every hour"
msgstr "Una volta ogni ora"

msgctxt "selection_freq_value_two"
msgid "Every two hours"
msgstr "Ogni due ore"

msgctxt "selection_freq_value_three"
msgid "Every three hours"
msgstr "Ogni tre ore"

msgctxt "selection_freq_value_four"
msgid "Every four hours"
msgstr "Ogni quattro ore"

msgctxt "vibrate_pref_title"
msgid "Vibrate enabled"
msgstr "Vibrazione abilitata"

#, fuzzy
msgctxt "vibrate_pref_summary"
msgid "Choose whether your phone should also vibrate when stuff happens."
msgstr "Scegli se il telefono deve vibrare alla ricezione di un evento."

msgctxt "distort_pos_pref_title"
msgid "Distort location"
msgstr "Falsare localitá"

msgctxt "distort_pos_pref_summary"
msgid "If you have privacy concerns, you can force an inaccurate location."
msgstr "Se avete problemi di privacy, è possibile forzare una posizione imprecisa."

msgctxt "distort_pos_value_do_not"
msgid "Do not distort"
msgstr "Non falsare"

msgctxt "distort_pos_value_200m"
msgid "200 meters"
msgstr "200 metri"

msgctxt "distort_pos_value_500m"
msgid "500 meters"
msgstr "500 metri"


msgid "200 meters"
msgstr "200 metros"

msgctxt "distort_pos_value_500m"
msgid "500 meters"
msgstr "500 metros"

msgctxt "distort_pos_value_1km"
msgid "1 kilometer"
msgstr "1 quilómetro"

msgctxt "distort_pos_value_5km"
msgid "5 kilometers"
msgstr "5 quilómetros"

msgctxt "distort_pos_value_10km"
msgid "10 kilometers"
msgstr "10 quilómetros"

msgctxt "pref_cat_sounds"
msgid "Notifications and Sounds"
